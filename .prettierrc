{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "endOfLine": "lf",
  "useTabs": false,
  "bracketSpacing": true,
  "printWidth": 80,
  "whitespace": [
    true,
    "check-branch",
    "check-decl",
    "check-operator",
    "check-module",
    "check-separator",
    "check-rest-spread",
    "check-type",
    "check-typecast",
    "check-type-operator",
    "check-preblock"
  ]
}
