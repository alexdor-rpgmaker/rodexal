{
  "name": "rodexal",
  "type": "project",
  "description": "New Alex d'or Website",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "CC",
  "require": {
    "php": "^7.1.3",
    "ext-intl": "*",
    "cviebrock/eloquent-sluggable": "^4.6",
    "fideloper/proxy": "^4.0",
    "genert/bbcode": "^1.1",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": "5.8.*",
    "laravel/tinker": "^1.0",
    "league/oauth2-client": "^2.4"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "heroku/heroku-buildpack-php": "^161.0",
    "laravel/dusk": "^5.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0.1",
    "phpunit/phpunit": "^8.3",
    "spatie/phpunit-watcher": "^1.8"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit --testdox",
    "test:watch": "phpunit-watcher watch --testdox",
    "test:e2e": " php artisan dusk --testdox",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
