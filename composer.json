{
  "name": "alexdor-rpgmaker/rodexal",
  "type": "project",
  "description": "Version 2019-? du site des Alex d'or",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "CC",
  "require": {
    "php": "^7.4|^8.0",
    "ext-intl": "*",
    "ext-json": "*",
    "cviebrock/eloquent-sluggable": "^8.0",
    "doctrine/dbal": "^2.10",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "genert/bbcode": "^1.1",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^8.12",
    "laravel/legacy-factories": "^1.1",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.0",
    "league/oauth2-client": "^2.4",
    "spatie/laravel-feed": "^3.1"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "facade/ignition": "^2.5",
    "filp/whoops": "^2.0",
    "fakerphp/faker": "^1.9.1",
    "heroku/heroku-buildpack-php": "^190.0",
    "laravel/dusk": "^6.0",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3",
    "spatie/phpunit-watcher": "^1.8"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "process-timeout": 600
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit --testdox",
    "test:watch": "phpunit-watcher watch --testdox",
    "test:e2e": "php artisan dusk --testdox",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
